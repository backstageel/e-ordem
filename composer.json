{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/backstageel/livewire-alert"
        },
        {
            "type": "vcs",
            "url": "https://github.com/backstageel/livewire-powergrid"
        },
        {
            "type": "vcs",
            "url": "https://github.com/backstageel/laravel-livewire-wizard"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hostmoz/blade-bootstrap-components"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php": "^3.322",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "cknow/laravel-money": "^8.4",
        "dedoc/scramble": "^0.12.19",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "fakerphp/faker": "^1.23",
        "hostmoz/blade-bootstrap-components": "dev-main",
        "intervention/image": "^3.11",
        "jantinnerezo/livewire-alert": "dev-livewire-v4beta",
        "laracasts/flash": "^3.2",
        "laravel/folio": "^1.1",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.7",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^4.0@beta",
        "lorisleiva/laravel-actions": "^2.9",
        "maatwebsite/excel": "^3.1",
        "mhmiton/laravel-modules-livewire": "^1.6",
        "nwidart/laravel-modules": "^12.0",
        "nyholm/psr7": "^1.8",
        "openai-php/client": "^0.17.0",
        "openspout/openspout": "^4.0",
        "owen-it/laravel-auditing": "^14.0",
        "power-components/livewire-powergrid": "dev-livewire-4.0beta",
        "pragmarx/google2fa": "^9.0",
        "pragmarx/google2fa-laravel": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-data": "^4.13",
        "spatie/laravel-livewire-wizard": "dev-main",
        "spatie/laravel-medialibrary": "^11.17",
        "spatie/laravel-permission": "^6.16",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-tags": "^4.10",
        "symfony/http-client": "^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-debugbar": "^3.13",
        "beyondcode/laravel-er-diagram-generator": "^5.0",
        "deployer/deployer": "^7.4",
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.3",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-browser": "^4.1",
        "pestphp/pest-plugin-drift": "^4.0",
        "phpunit/phpunit": "^11.5.3",
        "recca0120/laravel-erd": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\States\\": "database/States/"
        },
        "files": [
            "app/helpers.php"
        ],
        "exclude-from-classmap": [
            "vendor/laravel/pint/app/Providers/",
            "vendor/league/flysystem-local/",
            "vendor/league/flysystem/src/Local/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan boost:update --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "source",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}